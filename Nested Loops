/*Output: 12 
          34  
          56*/ 

#include <iostream> 
using namespace std;  

int main() 
{ 
    int numberGrid[3][2] =    
    { 
        {1, 2},   
        {3, 4},   
        {5, 6}    
    };  
    
    for(int i = 0; i < 3; i++)   // Each row will be accounted for in the 2D array.  
    { 
        for(int j = 0; j < 2; j++)   // Each column will be accounted for in the 2D array.  
        { 
            cout << numberGrid[i][j];   // The row of an array element is set to i. The column of an array element is set to j.  
        } 
        cout << endl;  
    } 
    
    return 0; 
} 
